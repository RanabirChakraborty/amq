{% set accesses = [] %}
{% for method in default_access.methods %}
  {% set rolesInput=default_access.roles | union(activemq_hawtio_role) | unique | sort | join(',') %}
  {% set accessElem = { 'method': method, 'roles': rolesInput } %}
  {% set _ = accesses.append( { 'access': accessElem } ) %}
{% endfor %}

{{ accesses }}
